[SERVER]
11.11.11.11

[DOWNLOAD]
gtak/4.0.0 https://github.com/broadinstitute/gatk/releases/download/4.0.0.0/gatk-4.0.0.0.zip

[DEPENDENCY]

[ENV]
module purge
module use ${JARVIS_ROOT}/software/modulefiles
module add bisheng2/2.1.0
export CC=clang CXX=clang++ FC=flang

[APP]
app_name = gatk
build_dir = ${JARVIS_ROOT}
binary_dir = 
case_dir = 

[BUILD]
./jarvis -install gatk/4.0.0.0 clang

[RUN]
run = 
binary = 
node = 1
